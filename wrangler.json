{
  "name": "pagezero",
  "compatibility_date": "2025-08-23",
  "compatibility_flags": ["nodejs_compat"],
  "main": "workers/index.ts",
  "assets": {
    "directory": "./build/client",
    "binding": "ASSETS"
  },
  "preview_urls": true,
  "observability": {
    "enabled": true
  },
  "upload_source_maps": true,
  "d1_databases": [
    {
      "binding": "DB",
      "database_name": "xianglu-development",
      "database_id": "<DATABASE_ID>"
    }
  ],
  "env": {
    "production": {
      "d1_databases": [
        {
          "binding": "DB",
          "database_name": "xianglu-production",
          "database_id": "2d718bdc-6524-4d42-a974-3862600fe98a"
        }
      ]
    },
    "preview": {
      "d1_databases": [
        {
          "binding": "DB",
          "database_name": "xianglu-preview",
          "database_id": "0c0470f7-91f8-4d8e-90e2-f6ce3f061149"
        }
      ]
    },
    "test": {
      "d1_databases": [
        {
          "binding": "DB",
          "database_name": "xianglu-development",
          "database_id": "<DATABASE_ID>"
        }
      ]
    }
  }
}
